{
  "name": "@citolab/tspci-3d-blocks",
  "source": "src/index.tsx",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "description": "recreate a building with 3d blocks",
  "private": true,
  "dependencies": {
    "@citolab/preact-store": "2.6.8",
    "preact": "10.28.2",
    "three": "^0.136.0",
    "three-orbitcontrols": "^2.110.3"
  },
  "devDependencies": {
    "@citolab/tspci": "2.7.1",
    "@citolab/tspci-tao": "2.7.1",
    "@citolab/tspci-qti3": "2.7.1",
    "@types/three": "^0.136.0",
    "archiver": "7.0.1",
    "autoprefixer": "10.4.23"
  },
  "scripts": {
    "dev": "set ROLLUP_WATCH=true && tspci --dev",
    "prod": "tspci",
    "watch": "tspci --watch",
    "package-tao": "tspci --target tao",
    "package-qti3": "tspci --target qti3"
  },
  "config": {
    "tspci": {
      "typeIdentifier": "blocks",
      "label": "3d blocks",
      "score": [
        "MATCH_CORRECT",
        "NONE"
      ],
      "short": "blocks"
    }
  }
}
