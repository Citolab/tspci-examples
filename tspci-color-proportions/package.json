{
  "name": "@citolab/pci-verhoudingen",
  "version": "1.0.1",
  "description": "color the proportions of the colors in the image",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "dependencies": {
    "@citolab/preact-store": "2.6.8",
    "@react-hook/mouse-position": "^4.1.3",
    "preact": "10.28.2"
  },
  "devDependencies": {
    "@citolab/tspci": "2.7.1",
    "@citolab/tspci-tao": "2.7.1",
    "@citolab/tspci-qti3": "2.7.1",
    "@tailwindcss/postcss": "^4.1.18",
    "archiver": "7.0.1",
    "autoprefixer": "10.4.23",
    "tailwindcss": "^4.1.18"
  },
  "scripts": {
    "dev": "set ROLLUP_WATCH=true && tspci --dev",
    "prod": "tspci",
    "watch": "tspci --watch",
    "package-tao": "tspci --target tao",
    "package-qti3": "tspci --target qti3"
  },
  "config": {
    "tspci": {
      "typeIdentifier": "colorProportions",
      "label": "verhoudingen",
      "score": [
        "MATCH_CORRECT",
        "NONE"
      ],
      "short": "verhoudingen"
    }
  }
}
